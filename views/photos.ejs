<body class = 'photos'>
  <h2 class = 'mediumFont flex justifyContent'>Click To View Full Image!</h2>

    <section class ='flex alignItems column illya'>

        <% let index = 0; %> 

        <% for(let i=1; i < 19; i++) {%>

            <section class = 'flex' id = 'img<%=[i]%>'>
                <% index+=1; %> 
                <a target = '_blank' href = '/photos/<%= [index] %>.jpg'><img src = '/photos/<%= [index] %>.jpg' class = 'jpg<%= [index] %>'></a>
                <% index+=1; %> 
                <a target = '_blank' href = '/photos/<%= [index] %>.jpg'><img src = '/photos/<%= [index] %>.jpg' class = 'jpg<%= [index] %>'></a>
                <% index+=1; %> 
                <a target = '_blank' href = '/photos/<%= [index] %>.jpg'><img src = '/photos/<%= [index] %>.jpg' class = 'jpg<%= [index] %>'></a>
            </section>

            <% } %>


            <% let illya = 0; %> 

            <% for(let i=1; i < 6; i++) {%>
    
                <section class = 'flex' id = 'image<%=[i]%>'>
                    <% illya+=1; %> 
                    <a target = '_blank' href = '/photos/<%= [illya] %>.png'><img src = '/photos/<%= [illya] %>.png' class = 'png<%= [illya] %>'></a>
                    <% illya+=1; %> 
                    <a target = '_blank' href = '/photos/<%= [illya] %>.png'><img src = '/photos/<%= [illya] %>.png' class = 'png<%= [illya] %>'></a>
                    <% illya+=1; %> 
                    <a target = '_blank' href = '/photos/<%= [illya] %>.png'><img src = '/photos/<%= [illya] %>.png' class = 'png<%= [illya] %>'></a>
                </section>
    
                <% } %>

    </section>
</body>

